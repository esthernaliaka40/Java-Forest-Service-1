<div class="container">
<div class="jumbotron">
 <h1 class="text-center">The Sightings</h1>
     <table class="table table-striped table-hover">
         <thead class="table-dark">
         <tr>
             <th scope="col">Animal Name</th>
             <th scope="col">Ranger's Name</th>
             <th scope="col">Location</th>
             <th scope="col">Health</th>
             <th scope="col">Age</th>
             <th scope="col">Animal Type</th>
         </tr>
         </thead>
         <tbody>
         {{#each sightings}}
             <tr>
                 <td>{{name}}</td>
                 <td>{{animalRangerName}}</td>
                 <td>{{location}}</td>
                 <td>{{health}}</td>
                 <td>{{age}}</td>
                 <td>{{species}}</td>
             </tr>
         {{/each}}
         </tbody>
     </table>
</div>
</div>